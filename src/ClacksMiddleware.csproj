<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>A .NET Core Middleware which adds the GNU Terry Pratchett http header to all responses.</Description>
    <VersionPrefix>1.0.0.16</VersionPrefix>
    <Authors>Jamie Taylor</Authors>
    <AssemblyName>ClacksMiddlware</AssemblyName>
    <TargetFramework>netstandard1.4</TargetFramework>
    <PackageId>ClacksMiddlware</PackageId>
    <RuntimeFrameworkVersion>1.1.0</RuntimeFrameworkVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="1.1.*" />
  </ItemGroup>
</Project>